    "prettier:check": "prettier --check \"./src/**/*.{js,jsx,json,ts,tsx}\"",
    "prettier:fix": "prettier --write \"./src/**/*.{js,jsx,json,ts,tsx}\"",
    "stylelint": "stylelint src/**/*{.scss,.css}",
    "stylelint:fix": "stylelint src/**/*{.scss,.css} --fix",
    "preview": "vite preview"
